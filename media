#include <stdio.h>

int main()
{

   

	float nota1, nota2, nota3, nota4, media  
	printf("insira a nota1 : ");
	scanf("%f",&nota1);
	printf("\ninsira a nota2  : ");
	scanf("%f",&nota2);
	printf("\ninsira a nota3 : ");
	scanf("%f",&nota3);
	printf("\ninsira a nota4 : ");
	scanf("%f",&nota4);

	media = (nota1 + nota2 + nota3 + nota4 ) / 4;
	printf("a média é %.2f",media);
	        return 0 ;
}
