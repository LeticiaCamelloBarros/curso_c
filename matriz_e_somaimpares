#include <stdio.h>

int main()
{
    
    int matriz[3][3];
    int i = 0 ;
    int j = 0 ;
    int soma_impares = 0 ;
    for (i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d",&matriz[i][j]);
            printf(" ");
        }
        printf("\n");
    }

    for (i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d",matriz[i][j]);
            printf(" ");
            if( matriz[i][j] % 2 != 0) 
            {
                soma_impares += matriz[i][j];
            }
        }
        printf("\n");
    }
    printf("a soma dos números impares é %d",soma_impares);
}
